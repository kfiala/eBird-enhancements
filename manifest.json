{
  "manifest_version": 3,
  "name": "eBird trip report utilities",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://ebird.org/*tripreport/*"],
      "js": ["tripreport.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "tripreport@faintlake",
      "strict_min_version": "109.0"
    }
  },
  "description": "Utilities for eBird trip reports.",
  "homepage_url": "https://www.faintlake.com/eBird/extension/TripReport/",
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png"
  },
  "author": "Kent Fiala"
}
