{
  "manifest_version": 3,
  "name": "eBird assistant",
  "version": "3.1.8",
  "content_scripts": [
    {
      "matches": [
        "https://ebird.org/*",
        "https://media.ebird.org/*"
      ],
      "js": ["autofocus.js","commonFunctions.js","downloadTrack.js","tripreport.js","mychecklists.js","checklistSettingsButton.js","regionPage.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "tripreport@faintlake",
      "strict_min_version": "109.0"
    },
    "gecko_android": {
      "strict_min_version": "113.0"
    }
  },
  "description": "Enhancements for the eBird web interface",
  "homepage_url": "https://www.faintlake.com/eBird/extension/Enhancements/",
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png"
  },
  "author": "Kent Fiala"
}
