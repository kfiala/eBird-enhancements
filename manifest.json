{
  "manifest_version": 3,
  "name": "eBird enhancements",
  "version": "3.1.2",
  "content_scripts": [
    {
      "matches": ["https://ebird.org/*checklist/*","https://ebird.org/*tripreport/*","https://ebird.org/*mychecklists*","https://ebird.org/region/*","https://ebird.org/hotspot/*"],
      "js": ["commonFunctions.js","downloadTrack.js","tripreport.js","mychecklists.js","shareLink.js","checklistSettingsButton.js","regionPage.js"]
    }
  ],
  "description": "Enhancements for the eBird web interface",
  "homepage_url": "https://www.faintlake.com/eBird/extension/Enhancements/",
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png"
  },
  "author": "Kent Fiala"
}
